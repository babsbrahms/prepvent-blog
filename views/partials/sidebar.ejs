                    <div class="sidebar_widget">
                        <div class="single_sidebar_wiget search_form_widget">
                            <form id="sidebar-search-form" autocomplete="on">
                                <div class="form-group">
                                    <input id="sidebar-search-input" type="text" class="form-control" placeholder='Search Keyword'
                                        onfocus="this.placeholder = ''" onblur="this.placeholder = 'Search Keyword'">
                                        <ul class="list-group" id="sidebar-search-ul"></ul>
                                    <div id='sidebar-search-btn' class="btn_1">search</div>
                                </div>
                            </form>
                            
                        </div>
                        <script>
                            let searchBtn= document.getElementById('sidebar-search-btn')
                            let searchInput = document.getElementById('sidebar-search-input');
                            let form = document.getElementById('sidebar-search-form')
                            searchBtn.addEventListener('click', searchBlog)

                            form.onsubmit = function (e) {
                                e.preventDefault;
                                console.log('hi');
                                searchBlog(e)
                                return false;
                            }

                            function searchBlog (e) {
                                e.preventDefault()
                                fetch(`/search?search=${searchInput.value}`, {
                                    method: 'GET'
                                })
                                .then(res => res.json())
                                .then(res =>{

                                    let Card = ''
                                    res.result.forEach(element => 
                                        Card += `<li class="list-group-item text-center"> <a href="/blog/${element._id}">${element.title} </a></li>`
                                    );
                                    
                                    document.getElementById('sidebar-search-ul').innerHTML = Card ;
                                })
                                .catch(err => {

                                })

                               
                            }

                            searchInput.onkeyup = function (e) {
                                
                                if (e.target.value <= 1) {
                                    document.getElementById('sidebar-search-ul').innerHTML = ''
                                }
                            }

                        </script>
                        <div class="single_sidebar_wiget">
                            <div class="sidebar_tittle">
                                <h3>Categories</h3>
                            </div>
                            <div class="single_catagory_item category">
                                <ul class="list-unstyled">
                                    <%categories.forEach(function(category){%> 
                                        <li><a style="width: 100%;" href="/category/<%=category%>"><%=category%></a></li>
                                    <%})%>
                                </ul>
                            </div>
                        </div>
